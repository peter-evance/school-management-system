{
    "builds": [{
        "src": "school_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize":"15mb", "runtime":"python3.11" }
    }],

    "routes": [
        {
            "src": "/(.*)",
            "dest": "school_management_system/wsgi.py"
        }
    ]
}